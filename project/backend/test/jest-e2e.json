{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testMatch": ["**/*.e2e-spec.ts"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [],
  "moduleNameMapper": {
    "^prisma/(.*)$": "<rootDir>/../prisma/$1",
    "^src/(.*)$": "<rootDir>/../src/$1",
    "^@/(.*)$": "<rootDir>/../src/$1"
  }
}
